#pragma once

#include "model.qh"

REGISTRY(Models, BITS(9))
REGISTER_REGISTRY(Models)

#define MODEL(name, thepath) \
    string MDL_##name##_get() { return thepath; } \
    REGISTER(Models, MDL, name, m_id, NEW(Model, MDL_##name##_get))

